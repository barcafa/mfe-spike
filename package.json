{
  "name": "mfe-spike",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start:all": "nx run-many --target serve --all --maxParallel 2",
    "start:shell": "ng serve shell  -o --port 5000",
    "start:admin": "ng serve admin -o --port 3000",
    "build:shell": "ng build shell --configuration production",
    "build:admin": "ng build admin --configuration production",
    "serve:dist": "concurrently \"serve dist/shell -l 5000 -s\" \"serve dist/admin -l 3000 -s\" ",
    "start": "concurrently \"npm run start:shell\" \"npm run start:admin\" ",
    "build": "npm run build:shell && npm run build:admin",
    "lint": "nx workspace-lint && ng lint",
    "test": "nx test",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "dep-graph": "nx dep-graph",
    "postinstall": "ngcc",
    "postinstall:bak": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "build:shell:externals": "ng build --extra-webpack-config apps/shell//webpack.externals.js --prod --project shell --single-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.2.1",
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@nrwl/angular": "13.9.4",
    "concurrently": "^5.3.0",
    "ngx-build-plus": "^13.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "~13.2.0",
    "@nrwl/cli": "13.8.8",
    "@nrwl/cypress": "13.9.4",
    "@nrwl/eslint-plugin-nx": "13.9.4",
    "@nrwl/jest": "13.9.4",
    "@nrwl/linter": "13.9.4",
    "@nrwl/workspace": "13.9.4",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "cypress": "^9.1.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2",
    "nx": "13.9.4"
  },
  "resolutions": {
    "webpack": "5.0.0"
  }
}